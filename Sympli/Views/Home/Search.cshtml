    <div class="container-fluid">


    <div class="form-group table-responsive-sm table-primary">
        <div class="form-group form-row">
            <div class="col-10 align-content-center"> Search results scraped from different search engines </div>
        </div>
        <div class="form-group form-row">
            <div class="col-6 align-content-sm-end">Search Engine Type</div>
            <div class="col-6 align-content-sm-start">Number of appearences</div>
        </div>
        @foreach (var result in ViewBag.SearchResults)
        {
            <div class="form-group form-row">
                <div class="col-6 align-content-sm-end">@result.SearchEngine</div>
                <div class="col-6 align-content-sm-start">@result.Appearences</div>
            </div>
        }
    </div>

    <div class="form-group form-row">
        <div class="col-12">
            @Html.ActionLink("Back", "Index", "Home")
        </div>
    </div>

</div>
        